<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seedu.canvas.controller.CanvasController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: white;" StackPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" fillHeight="false" minHeight="30.0" prefHeight="30.0" prefWidth="200.0" style="-fx-background-color: #203864;" VBox.vgrow="NEVER">
               <children>
                  <Label fx:id="titleLabel" text="Can/Vas" />
               </children></HBox>
            <Separator prefWidth="200.0" />
            <HBox fx:id="toolBox" alignment="CENTER_LEFT" minHeight="100.0" prefHeight="100.0" prefWidth="200.0" spacing="5.0" VBox.vgrow="NEVER">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding></HBox>
            <Separator prefWidth="200.0" />
            <HBox alignment="CENTER" VBox.vgrow="SOMETIMES">
               <children>
                  <ScrollPane fitToHeight="true" fitToWidth="true" HBox.hgrow="SOMETIMES">
                     <content>
                        <VBox fx:id="canvasBox" alignment="CENTER" minHeight="600.0" minWidth="600.0" style="-fx-background-color: white;" />
                     </content>
                  </ScrollPane>
                  <Separator orientation="VERTICAL" prefHeight="200.0" />
                  <VBox alignment="CENTER" fillWidth="false" prefHeight="200.0" prefWidth="250.0" HBox.hgrow="NEVER" />
               </children>
            </HBox>
         </children></VBox>
   </children>
</StackPane>
